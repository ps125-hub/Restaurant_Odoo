{
    "version": "0.2.0",
    "configurations": [
    
    
        {
            "name": "Python:Odoo",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "python": "${command:python.interpreterPath}",
            "console": "integratedTerminal",
            "program": "/usr/bin/odoo",
            "args": [
                "--config=/etc/odoo/odoo.conf",
            ],
            "cwd": "${workspaceRoot}",
            "env": {},
            "envFile": "${workspaceRoot}/.env",
        }
    ]
}